<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="DashboardCruiseListPage.css" />
		
	<g:HTMLPanel>
		<div styleName="{style.header-div}">
			<g:InlineLabel styleName="{style.welcome-label}" ui:field="welcomeLabel" />
			<g:Button styleName="{style.logout-button}" ui:field="logoutButton" />
		</div>
		<div styleName="{style.contents}">
			<g:Label styleName="{style.dummy-label}">Hi There!</g:Label>
		</div>
	</g:HTMLPanel>

</ui:UiBinder> 