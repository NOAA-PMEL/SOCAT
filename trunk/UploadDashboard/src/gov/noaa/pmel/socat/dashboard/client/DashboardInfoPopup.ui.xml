<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="FixedWidth.css" />

	<g:VerticalPanel addStyleNames="{style.infopanel}" horizontalAlignment="ALIGN_CENTER">
		<g:HTML addStyleNames="{style.infohtml}" ui:field="infoHTML" />
		<g:Button addStyleNames="{style.dismissbutton}" ui:field="dismissButton" />
	</g:VerticalPanel>

</ui:UiBinder> 