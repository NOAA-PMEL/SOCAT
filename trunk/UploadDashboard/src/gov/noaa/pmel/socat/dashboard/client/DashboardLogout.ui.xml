<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="DashboardLogout.css"/>

	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:HorizontalPanel addStyleNames="{style.intro-panel}">
				<g:Label addStyleNames="{style.intro-label}" ui:field="goodbyeLabel" />
			</g:HorizontalPanel>
			<g:HorizontalPanel addStyleNames="{style.relogin-panel}">
				<g:Button addStyleNames="{style.relogin-button}" ui:field="reloginButton" />
			</g:HorizontalPanel>
			<g:HorizontalPanel addStyleNames="{style.socat-info-panel}">
				<g:Anchor addStyleNames="{style.socat-info-anchor}" ui:field="socatInfoAnchor" />
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>

</ui:UiBinder> 