<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="CommonFixedWidth.css"/>

	<g:VerticalPanel addStyleNames="{style.fixedwidthcontainer}" horizontalAlignment="ALIGN_CENTER">
		<g:HTML addStyleNames="{style.introhtml}" ui:field="goodbyeHTML" />
		<g:Button addStyleNames="{style.loginbutton}" ui:field="reloginButton" />
		<g:HorizontalPanel addStyleNames="{style.socatinfopanel}">
			<g:Anchor addStyleNames="{style.socatinfoanchor}" ui:field="socatInfoAnchor" />
		</g:HorizontalPanel>
	</g:VerticalPanel>

</ui:UiBinder> 