<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="DashboardLogin.css" />

	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:Label addStyleNames="{style.intro-label}" ui:field="welcomeLabel" />
			<g:HorizontalPanel addStyleNames="{style.labeledtext-panel}">
				<g:Label addStyleNames="{style.labeledtext-label}" ui:field="nameLabel" />
				<g:TextBox addStyleNames="{style.labeledtext-textbox}" ui:field="nameText" />
			</g:HorizontalPanel>
			<g:HorizontalPanel addStyleNames="{style.labeledtext-panel}">
				<g:Label addStyleNames="{style.labeledtext-label}" ui:field="passLabel" />
				<g:PasswordTextBox addStyleNames="{style.labeledtext-textbox}" ui:field="passText" />
			</g:HorizontalPanel>
			<g:Button addStyleNames="{style.login-button}" ui:field="loginButton" />
		</g:VerticalPanel>
	</g:HTMLPanel>

</ui:UiBinder> 