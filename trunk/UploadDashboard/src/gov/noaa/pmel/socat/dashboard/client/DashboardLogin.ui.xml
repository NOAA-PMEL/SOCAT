<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="DashboardLogin.css" />
		
	<g:HTMLPanel>
		<div styleName="{style.intro-div}">
			<g:Label styleName="{style.intro-label}" ui:field="welcomeLabel" />
		</div>
		<div styleName="{style.labeledtext-div}">
			<g:InlineLabel styleName="{style.labeledtext-label}" ui:field="nameLabel" />
			<g:TextBox styleName="{style.labeledtext-textbox}" ui:field="nameText" />
		</div>
		<div styleName="{style.labeledtext-div">
			<g:InlineLabel styleName="{style.labeledtext-label}" ui:field="passLabel" />
			<g:PasswordTextBox styleName="{style.labeledtext-textbox}" ui:field="passText" />
		</div>
		<div styleName="{style.buttons-div}">
			<g:Button styleName="{style.login-button}" ui:field="loginButton" />
		</div>
	</g:HTMLPanel>

</ui:UiBinder> 